{
  "name": "da-admin",
  "version": "1.1.0",
  "description": "",
  "main": "index.js",
  "type": "module",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "prepare": "husky",
    "build:docs": "npx @redocly/cli build-docs -o docs/index.html admin@v1",
    "watch:docs": "npx @redocly/cli preview-docs admin@v1",
    "lint": "eslint . --ext js,cjs,mjs",
    "test": "c8 mocha --spec=test/**/*.test.js",
    "test:e2e": "mocha --spec=test/e2e/**/*.spec.js",
    "docker:up": "docker compose -f test/docker-compose.yaml up -d",
    "docker:down": "docker compose -f test/docker-compose.yaml down",
    "start": "npm run dev",
    "dev": "if test -e .dev.vars; then wrangler dev --env dev --; else echo \"Need a .dev.vars files before starting local dev server\"; fi",
    "deploy:stage": "node prepare-deploy.js && wrangler deploy -e stage -c wrangler-versioned.toml",
    "deploy:production": "node prepare-deploy.js && wrangler deploy -c wrangler-versioned.toml",
    "semantic-release": "semantic-release",
    "semantic-release-dry": "semantic-release --dry-run --no-ci --branches $CI_BRANCH"
  },
  "author": "",
  "repository": {
    "type": "git",
    "url": "https://github.com/adobe/da-admin"
  },
  "license": "Apache-2.0",
  "mocha": {
    "require": [
      "nock"
    ]
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.456.0",
    "@aws-sdk/s3-request-presigner": "^3.468.0",
    "@ssttevee/cfw-formdata-polyfill": "^0.2.1",
    "jose": "^5.9.6"
  },
  "devDependencies": {
    "@adobe/eslint-config-helix": "2.0.6",
    "@redocly/cli": "^1.4.1",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.1",
    "@semantic-release/exec": "^7.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^11.0.1",
    "@semantic-release/npm": "^12.0.1",
    "@semantic-release/release-notes-generator": "^14.0.3",
    "aws-sdk-client-mock": "^4.0.0",
    "c8": "^8.0.1",
    "eslint": "8.56.0",
    "esmock": "^2.6.4",
    "husky": "^9.1.7",
    "lint-staged": "^15.4.3",
    "mocha": "^10.2.0",
    "nock": "^14.0.1",
    "semantic-release": "^24.2.2",
    "wrangler": "^3.107.3"
  },
  "lint-staged": {
    "*.(js|cjs|mjs)": "eslint"
  }
}
